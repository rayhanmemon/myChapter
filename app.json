{
  "expo": {
    "name": "myChapter",
    "slug": "myChapter",
    "android": {
      "package": "com.myChapter.rayhanmemon",
      "versionCode": 1,
      "androidStatusBar": {
          "backgroundColor": "#ff0000",
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ed1c24"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "6",
      "bundleIdentifier": "com.myChapter.myChapter",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your current location will be used when on the Events Tab to check if you are in range of an event. This is necessary for you to check in to events."
      },
    },
  }
}
